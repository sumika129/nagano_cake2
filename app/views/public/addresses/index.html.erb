<h3>配送先登録／一覧</h3>

<%= form_with model: [:public,@address] do |f| %>
<table>
  <tr>
    <th>郵便番号（ハイフンなし）</th>
    <td><%= f.text_field :postal_code %></td>
  </tr>
  <tr>
    <th>住所</th>
    <td><%= f.text_field :address %></td>
  </tr>
  <tr>
    <th>宛名</th>
    <td><%= f.text_field :name %></td>
    <td><%= f.submit '新規登録' %></td>
  </tr>
</table>
<% end %>

